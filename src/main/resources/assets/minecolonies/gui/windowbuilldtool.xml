        <view size="100% 15">
            <label id="id" size="100% 100%" label="" pos="0 0"/>
            <button id="button" size="100% 100%" label="" pos="0 0"/>
        </view>
    </dropdown>
    <button id="nextBuildingType" size="15 15" pos="125 0" label="&gt;"/>

    <button id="previousStyle" size="15 15" pos="140 0" label="&lt;"/>
