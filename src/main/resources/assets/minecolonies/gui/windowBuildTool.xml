<window xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" size="420 240" pause="true"
        lightbox="false"
        xsi:noNamespaceSchemaLocation="file:../../../../java/com/minecolonies/blockout/blockOut.xsd">

    <button id="previousBuildingType" size="15 15" pos="0 0" label="&lt;"/>
    <dropdown id="buildingType" size="114 15" pos="13 0" dropDownSize="140 75">
        <view size="100% 15">
            <label id="id" size="100% 100%" label="" pos="0 0"/>
            <button id="button" size="100% 100%" label="" pos="0 0"/>
        </view>
    </dropdown>
    <button id="nextBuildingType" size="15 15" pos="125 0" label="&gt;"/>

    <button id="previousStyle" size="15 15" pos="140 0" label="&lt;"/>
    <dropdown id="style" size="114 15" pos="153 0" maxContentHeight="75" dropDownSize="140 75">
        <view size="100% 15">
            <label id="id" size="100% 100%" label="" pos="0 0"/>
            <button id="button" size="100% 100%" label="" pos="0 0"/>
        </view>
    </dropdown>
    <button id="nextStyle" size="15 15" pos="265 0" label="&gt;"/>

    <button id="previousSchematic" size="15 15" pos="280 0" label="&lt;"/>
    <dropdown id="schematic" size="114 15" pos="293 0" maxContentHeight="75" dropDownSize="140 75">
        <view size="100% 15">
            <label id="id" size="100% 100%" label="" pos="0 0"/>
            <button id="button" size="100% 100%" label="" pos="0 0"/>
        </view>
    </dropdown>
    <button id="nextSchematic" size="15 15" pos="405 0" label="&gt;"/>

    <button id="rename" size="114 15" pos="293 20" label="Rename" visible="false"/>
    <button id="delete" size="114 15" pos="293 40" label="Delete" visible="false"/>

    <view size="60 112" pos="180 120">
        <buttonimage id="rotateLeft"  source="minecolonies:textures/gui/rotateLeft-20px.png"  size="20" pos="0 0"/>
        <buttonimage id="forward"     source="minecolonies:textures/gui/up-20px.png"          size="20" pos="20 0"/>
        <buttonimage id="rotateRight" source="minecolonies:textures/gui/rotateRight-20px.png" size="20" pos="40 0"/>
        <buttonimage id="left"        source="minecolonies:textures/gui/left-20px.png"        size="20" pos="0 20"/>
        <buttonimage id="mirror"      source="minecolonies:textures/gui/mirror-20px.png"      size="20" pos="20 20"/>
        <buttonimage id="right"       source="minecolonies:textures/gui/right-20px.png"       size="20" pos="40 20"/>
        <buttonimage id="down"        source="minecolonies:textures/gui/minus-20px.png"       size="20" pos="0 40"/>
        <buttonimage id="backward"    source="minecolonies:textures/gui/down-20px.png"        size="20" pos="20 40"/>
        <buttonimage id="up"          source="minecolonies:textures/gui/plus-20px.png"        size="20" pos="40 40"/>
        <buttonimage id="confirm"     source="minecolonies:textures/gui/confirm.png"          size="32" pos="0 60"/>
        <buttonimage id="cancel"      source="minecolonies:textures/gui/cancel.png"           size="32" pos="28 60"/>
    </view>
</window>
