<window xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" size="420 240" pause="true"
        lightbox="false"
        xsi:noNamespaceSchemaLocation="file:../../../../java/com/minecolonies/blockout/blockOut.xsd">

    <buttonimage id="confirm" source="minecolonies:textures/gui/confirm.png" size="32" pos="90 100"/>
    <buttonimage id="cancel" source="minecolonies:textures/gui/cancel.png" size="32" pos="130 100"/>

    <buttonimage id="up" source="minecolonies:textures/gui/plus.png" size="32" pos="170 100"/>
    <buttonimage id="down" source="minecolonies:textures/gui/minus.png" size="32" pos="210 100"/>

    <buttonimage id="rotateLeft" source="minecolonies:textures/gui/rotateLeft.png" size="32" pos="250 100"/>
    <buttonimage id="rotateRight" source="minecolonies:textures/gui/rotateRight.png" size="32" pos="290 100"/>

    <buttonimage id="left" source="minecolonies:textures/gui/left.png" size="32" pos="130 140"/>
    <buttonimage id="right" source="minecolonies:textures/gui/right.png" size="32" pos="170 140"/>
    <buttonimage id="forward" source="minecolonies:textures/gui/up.png" size="32" pos="210 140"/>
    <buttonimage id="back" source="minecolonies:textures/gui/down.png" size="32" pos="250 140"/>

    <button id="hutDec" size="100 15" pos="110 180"/>

    <button id="style" size="100 15" pos="110 200" visible="false"/>
    <button id="level" size="100 15" pos="210 200" visible="false"/>

    <button id="buildingType" label="$(com.minecolonies.coremod.gui.buildtool.hutDec)" size="100 15" pos="210 180"/>
</window>