<window xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" pause="false"
        lightbox="true" xsi:noNamespaceSchemaLocation="file:../../../../java/com/minecolonies/blockout/blockOut.xsd">
    <label size="100% 11" pos="0 25" textalign="TOP_MIDDLE" color="white"
           label="$(com.minecolonies.coremod.gui.scan.replace.title)"/>
    <input id="name" size="150 18" pos="135 40" maxlength="25"/>

    <itemicon id="resourceIconFrom" size="17 17" pos="0 1"/>
    <label id="resourceNameFrom" size="140 12" pos="30 4" color="white"/>

    <label id="to" label="->" size="140 12" pos="170 4" color="white"/>

    <itemicon id="resourceIconTo" size="17 17" pos="210 1"/>
    <label id="resourceNameTo" size="140 12" pos="240 4" color="white"/>

    <list id="resources" size="300 120" pos="60 65">
        <box size="100% 17" linewidth="2">
            <itemicon id="resourceIcon" size="17 17" pos="1 1"/>
            <label id="resourceName" size="140 12" pos="30 1" color="white"/>
            <buttonimage label="$(com.minecolonies.coremod.gui.scanTool.select)" id="select" pos="160 1" size="86 17"
                         source="minecolonies:textures/gui/builderhut/builder_button_medium.png" textcolor="black"/>
        </box>
    </list>

    <buttonimage id="done" source="minecolonies:textures/gui/buildtool/confirm.png" size="32" pos="125 185"/>
    <buttonimage id="cancel" source="minecolonies:textures/gui/buildtool/cancel.png" size="32" pos="215 185"/>
</window>
