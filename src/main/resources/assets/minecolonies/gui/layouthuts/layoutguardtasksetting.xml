                 label="$(com.minecolonies.coremod.gui.workerhuts.targetpatrol)"
                 color="black"
                 pos="70 25"
                 size="80 17"
                 source="minecolonies:textures/gui/builderhut/builder_button_medium_large.png"/>
    <button id="helpButton"
                 label="?"
                 textalign="MIDDLE"
                 color="red"
                 pos="135 25"
                 size="15 15"
                 source="minecolonies:textures/gui/builderhut/builder_button_mini.png"/>
</window>
