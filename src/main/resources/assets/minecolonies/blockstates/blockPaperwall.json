{
  "multipart": [
    {   "apply": { "model": "minecolonies:blockPaperwall_post" }},
    {   "when": { "north": true },
      "apply": { "model": "minecolonies:blockPaperwall_side" }
    },
    {   "when": { "east": true },
      "apply": { "model": "minecolonies:blockPaperwall_side", "y": 90 }
    },
    {   "when": { "south": true },
      "apply": { "model": "minecolonies:blockPaperwall_side_alt" }
    },
    {   "when": { "west": true },
      "apply": { "model": "minecolonies:blockPaperwall_side_alt", "y": 90 }
    },
    {   "when": { "north": false },
      "apply": { "model": "minecolonies:blockPaperwall_noside" }
    },
    {   "when": { "east": false },
      "apply": { "model": "minecolonies:blockPaperwall_noside_alt" }
    },
    {   "when": { "south": false },
      "apply": { "model": "minecolonies:blockPaperwall_noside_alt", "y": 90 }
    },
    {   "when": { "west": false },
      "apply": { "model": "minecolonies:blockPaperwall_noside", "y": 270 }
    }
  ]
}
